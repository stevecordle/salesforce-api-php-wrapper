{
  "name": "stevecordle/salesforce-api-php-wrapper",
  "description": "A library for interacting with the Salesforce REST API and managing the OAuth flow",
  "keywords": ["salesforce", "rest", "api", "oauth2"],
  "homepage": "https://github.com/crunch-accounting/salesforce-api-php-wrapper",
  "license": "MIT",
  "authors": [
    {
      "name": "Arthur Guy",
      "email": "arthur.guy@crunch.co.uk"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "nesbot/carbon": "^1.12",
    "guzzlehttp/guzzle": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3.5"
  },
  "autoload": {
    "psr-4": {
      "Crunch\\Salesforce\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Crunch\\Salesforce\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable"
}
